pipeline{
 agent any 

 stages {
  stage ("Testing"){
     parallel {
        stage("unit test") {
          steps {
    echo 'unit test'
    sleep 5
  }
}
stage("integration test") {
  steps {
      echo 'integration test'
    sleep 5
  }
}
}
stage("unparallel test") {
  steps {
      echo 'unparallel test'
    sleep 5
  }
}
}
}
}
}
